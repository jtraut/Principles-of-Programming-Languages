// The following lines show how you can test your regular
// expression matcher.
console.log( /^a$/.test("a") )
console.log( /^a$/.test("b") )
console.log( /^aa$/.test("a") )
console.log( /^aa$/.test("aa") )
console.log( /^a|b$/.test("a") )
console.log( /^a|b$/.test("b") )
console.log( /^a*$/.test("") )
console.log( /^a*$/.test("aaa") )
console.log( /^a*$/.test("b") )

